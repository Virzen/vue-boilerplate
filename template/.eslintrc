{
	"parser": "babel-eslint",
    "rules": {
		"strict": 0,
        "indent": [
            1,
            "tab"
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ],
		"no-undef": 1,
		"no-unused-vars": 0,
		"comma-dangle": 0,
		"no-console": 0
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
